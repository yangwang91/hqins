page{
  border-top: 1px solid #eee;
}
.partership{
  padding:30rpx;
}
.partership_top{
  display: flex;
  align-items: center;
  justify-content: center;
}
.partership_top image{
  width:690rpx;
  height: 340rpx;
}
.case_tit{
  font-size:28rpx;
  font-weight: 500;
  padding-left: 16rpx;
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
}
.case_tit::before{
  content: ' ';
  position: absolute;
  width: 3rpx;
  height: 28rpx;
  background: #D3A468;
  left: 0;
  top: 36rpx;
}
.case_con{
  border:1px solid #D8D8D8;
  box-shadow:1px 0px 11px 2px rgba(216,216,216,0.36);
  border-radius:12rpx;
  padding: 20rpx;
}
.company{
  display: flex;
  align-items: top;
  padding: 10rpx 0 30rpx;
}
.company .left{
  width: 280rpx;
  height: 180rpx;
}
.company .left image{
  width: 100%;
  height: 100%;
}
.company .right{
  padding-left: 50rpx;
  position: relative;
}
.company .company_icon{
  position: absolute;
  top:5rpx;
  right: 0;
  height:38rpx;
  width: 38rpx;
}
.company .company_icon_r1{
  transform:rotate(-90deg);
}
.company .company_icon_r2{
  transform:rotate(90deg);
}
.company .company_icon image{
  height:100%;
  width: 100%;
}
.company .company_tit{
  font-size:32rpx;
  font-weight: 500;
  color:#000;
  padding-bottom: 20rpx;
  background-size:100% 100%;
}
.company .text{
  font-size:30rpx;
  font-weight: 400;
  color:#666;
}
.feature_tit{
  border-top: 1px dotted #D3A468;
  height: 80rpx;
  line-height: 80rpx;
}
.feature_tit::before{
  top:26rpx;
}
.company_jy{
  margin-top:-10rpx;
  font-size:28rpx;
  line-height: 1.8;
  padding:0 0 20rpx 15rpx;
  border-bottom: 1px dotted #D3A468;
}
.company_jy_tit{
  color:#D3A468
}
.company_desc{
  color:#333;
}
.company_detail{
  color:#535353;
  font-size:30rpx;
  font-weight: 400;
  line-height: 48rpx;
  padding-bottom: 100rpx;
}
.company_detail_img{
  display: flex;
  justify-content: center;
  align-items: center;
  padding-bottom: 20rpx;
}
.company_detail_img image{
  width:421rpx;
  height: 214rpx;
}
.case_company_bottom{
  margin:30rpx 0;
}
.case_company_bottom .company{
  padding: 10rpx 0 10rpx;
}