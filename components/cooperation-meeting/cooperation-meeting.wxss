/* components/cooperation-meeting/cooperation-meeting.wxss */

.cooperation-meeting {
  position: fixed;
  right: 12rpx;
  bottom: 250rpx;
  text-align: center;
  z-index: 10;
}

.cooperation-meeting_img {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 76rpx;
  height: 76rpx;
  margin: 0 auto;
  border-radius: 100%;
  background: #fff;
  border: 1rpx solid rgba(246, 246, 246, 1);
  box-shadow: 0rpx 6rpx 14rpx 0rpx rgba(48, 48, 48, 0.04);
}

.cooperation-meeting_img image {
  max-width: 60%;
  max-height: 60%;
}

.cooperation-meeting_text {
  width: 128rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 13rpx;
  font-size: 24rpx;
  text-align: center;
  border-radius: 20rpx;
  background: #fff;
  border: 1rpx solid rgba(246, 246, 246, 1);
  box-shadow: 0rpx 6rpx 14rpx 0rpx rgba(48, 48, 48, 0.04);
}
